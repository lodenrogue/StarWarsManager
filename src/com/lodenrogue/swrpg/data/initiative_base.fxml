<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox prefHeight="379.0" prefWidth="672.0" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.lodenrogue.swrpg.gmtools.initiative.InitiativeController">
   <children>
      <VBox prefHeight="339.0" prefWidth="407.0" spacing="10.0">
         <children>
            <ScrollPane layoutX="20.0" layoutY="50.0" prefHeight="302.0" prefViewportHeight="200.0" prefViewportWidth="313.0" prefWidth="384.0">
               <content>
                  <VBox fx:id="characterRowsBox" prefHeight="300.0" prefWidth="397.0" spacing="10.0">
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </VBox>
               </content>
            </ScrollPane>
            <HBox alignment="CENTER" prefHeight="2.0" prefWidth="368.0" spacing="100.0">
               <children>
                  <Button fx:id="addBtn" focusTraversable="false" mnemonicParsing="false" onAction="#onButtonPressed" prefWidth="68.0" text="Add" />
                  <Button fx:id="removeBtn" focusTraversable="false" mnemonicParsing="false" onAction="#onButtonPressed" prefWidth="68.0" text="Remove" />
               </children>
            </HBox>
         </children>
      </VBox>
      <VBox alignment="CENTER" prefHeight="311.0" prefWidth="206.0" spacing="50.0">
         <children>
            <Button fx:id="findInitiativeBtn" focusTraversable="false" mnemonicParsing="false" onAction="#onButtonPressed" text="Find Initiative">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <TextArea fx:id="infoArea" editable="false" focusTraversable="false" prefHeight="260.0" prefWidth="206.0" wrapText="true" />
         </children>
         <padding>
            <Insets top="20.0" />
         </padding>
      </VBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</HBox>
